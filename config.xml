<?xml version='1.0' encoding='utf-8'?>
<widget id="com.lewisnakao.html-css-js-template" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Ofudesaki Browser</name>
  <description>
  The html-css-js-template is an application this needs to be filled.
  </description>
  <author email="lewdev@gmail.com" href="http://lewdev.github.io">Lewis Nakao</author>
  <icon density="mdpi" src="www/img/ofudesaki-browser.png" width="200" height="200"/>
  <content src="index.html" />
  <plugin name="cordova-plugin-whitelist" spec="1" />
  <access origin="*" />
  <allow-intent href="https://fonts.googleapis.com/*" />
  <allow-intent href="https://fonts.gstatic.com/*" />
  <allow-intent href="mailto:*" />
  <!-- NOT RECOMMENDED
  <allow-navigation href="*" />
  -->
  <!--
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="geo:*" />
  -->
  <platform name="android">
    <allow-intent href="market:*" />
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
  </platform>
  <platform name="windows">
  </platform>
  <engine name="browser" spec="^5.0.4" />
  <engine name="windows" spec="^6.0.1" />
</widget>
